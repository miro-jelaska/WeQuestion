<!DOCTYPE html>
<html ng-app="app">
<head>
    <meta charset="utf-8" />
    <meta name="viewport" content="width=device-width" />
    <title>WeQuestion</title>
    <base href="/">
    <link rel="stylesheet" type="text/css" href="https://cdnjs.cloudflare.com/ajax/libs/normalize/4.2.0/normalize.min.css">
    <link rel="stylesheet" type="text/css" href="~/Scripts/libs/angular-ui-tree/angular-ui-tree.min.css">
    <style>
        .menu-option { display: inline-block; list-style: none;text-decoration: none; color: #2A2C2B }
        .survey-list-item:hover { background-color: #b0e0e6; }
        .angular-ui-tree-handle:hover { cursor: ns-resize; }
    </style>
</head>
<body>
    <ul class="menu">
        <li class="menu-option"><a ui-sref="admin.provisional">Provisional</a></li>
        <li class="menu-option"><a ui-sref="admin.open">Open</a></li>
        <li class="menu-option"><a ui-sref="admin.closed">Closed</a></li>
    </ul>

    <div ui-view></div>
    <script src="https://ajax.googleapis.com/ajax/libs/angularjs/1.5.7/angular.js" type="text/javascript"></script>
    <script src="https://cdnjs.cloudflare.com/ajax/libs/angular.js/1.5.7/angular-resource.js" type="text/javascript"></script>
    <script src="https://cdnjs.cloudflare.com/ajax/libs/angular-ui-router/0.3.1/angular-ui-router.js" type="text/javascript"></script>
    @Scripts.Render("~/bundles/libs")
    @Scripts.Render("~/bundles/app")
</body>
</html>
